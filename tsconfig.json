{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "esnext"],
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react",
      "declaration": true,
      "declarationMap": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "strict": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }
  